          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="btnGuardar" mnemonicParsing="false" prefHeight="75.0" prefWidth="112.0" text="Guardar" />
            <Button fx:id="btnVaciar" layoutX="10.0" layoutY="26.0" mnemonicParsing="false" prefHeight="75.0" prefWidth="112.0" text="Vaciar" GridPane.columnIndex="1" />
         </children>
      </GridPane>
      <Button fx:id="btnRegresar" layoutX="358.0" layoutY="665.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="276.0" text="Regresar" />
      <TableView fx:id="tblDistribuidores" layoutX="27.0" layoutY="175.0" prefHeight="431.0" prefWidth="651.0">
        <columns>
          <TableColumn fx:id="colDistribuidorId" prefWidth="61.0" text="Distribuidores ID" />
