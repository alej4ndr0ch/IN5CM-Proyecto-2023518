<AnchorPane id="AnchorPane" prefHeight="750.0" prefWidth="1200.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="org.alejandrocuxun.controllers.MenuDistribuidoresController">
   <children>
      <ImageView fitHeight="750.0" fitWidth="1200.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/Menu%20Distribuidores.png" />
         </image>
      </ImageView>
      <TextField fx:id="tfDistribuidorId" layoutX="780.0" layoutY="210.0" prefHeight="30.0" prefWidth="397.0" />
      <TextField fx:id="tfNombreDistribuidor" layoutX="780.0" layoutY="301.0" prefHeight="30.0" prefWidth="397.0" />
      <TextField fx:id="tfDescripcionDistribuidor" layoutX="709.0" layoutY="390.0" prefHeight="45.0" prefWidth="468.0" />
      <TextField fx:id="tfNit" layoutX="706.0" layoutY="492.0" prefHeight="30.0" prefWidth="209.0" />
      <TextField fx:id="tfTelefono" layoutX="964.0" layoutY="492.0" prefHeight="30.0" prefWidth="202.0" />
      <TextField fx:id="tfWeb" layoutX="767.0" layoutY="579.0" prefHeight="30.0" prefWidth="353.0" />
