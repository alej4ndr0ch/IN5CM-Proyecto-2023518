<AnchorPane id="AnchorPane" prefHeight="750.0" prefWidth="1200.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="org.alejandrocuxun.controllers.MenuDetalleFacturaController">
   <children>
      <ImageView fitHeight="750.0" fitWidth="1200.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/MenuDetalleFactura.png" />
         </image>
      </ImageView>
      <TextField fx:id="tfDetalleFacturaId" layoutX="14.0" layoutY="232.0" prefHeight="32.0" prefWidth="322.0" />
      <TextField fx:id="tfFacturaId" layoutX="14.0" layoutY="353.0" prefHeight="32.0" prefWidth="322.0" />
      <TextField fx:id="tfProductoId" layoutX="14.0" layoutY="462.0" prefHeight="32.0" prefWidth="322.0" />
      <TableView fx:id="tblDetalleFactura" layoutX="433.0" layoutY="156.0" prefHeight="499.0" prefWidth="718.0">
        <columns>
          <TableColumn fx:id="colDetalleFacturaId" prefWidth="226.0" text="DetalleFacturaId" />
          <TableColumn fx:id="colFacturaId" prefWidth="279.0" text="FacturaId" />
            <TableColumn fx:id="colProductoId" prefWidth="212.0" text="ProductoId" />
        </columns>
      </TableView>
      <GridPane layoutX="66.0" layoutY="544.0" prefHeight="84.0" prefWidth="219.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="btnGuardar" mnemonicParsing="false" prefHeight="69.0" prefWidth="106.0" text="Guardar" />
            <Button fx:id="btnVaciar" layoutX="10.0" layoutY="18.0" mnemonicParsing="false" prefHeight="69.0" prefWidth="106.0" text="Vaciar" GridPane.columnIndex="1" />
         </children>
      </GridPane>
      <Button fx:id="btnRegresar" layoutX="815.0" layoutY="684.0" mnemonicParsing="false" prefHeight="32.0" prefWidth="313.0" text="Regresar" />
   </children>
</AnchorPane>
